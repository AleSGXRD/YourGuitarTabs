<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <div class="container">
        <form action="/addTab" method="post">
            <% var count=12; for(var i=12 * pos;i<suggest.length&&count>0;i++) { %>
                <input type="hidden" name="id" id="id">

                <div
                    class="<%= i==0 + 12 * pos ?  'first' : i==suggest.length - 1 || (pos + 1) * 12 - 1==i ? 'last' : ''  %>">
                    <%= i+1 %>- <%= suggest[i].description %> -<%= suggest[i].direc%>
                                <a href="/add-tab/<%= suggest[i].id%>">add</a>
                </div>

                <% count--; } %>
        </form>
    </div>
</body>

</html>